#include <stdio.h>

/**
 * main - entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
	/**
	 * declarations
	 */
	int a, b, c, d;

	for (a = 0 ; a < 10 ; a++)
	{
	for (b = 0 ; b < 10 ; b++)
	{
		c = a + b;
		if (c > 9)
		{
			d = (c % 10);
			d = (z - u) / 10;
		}
	for (c = b + 1 ; c < 10 ; c++)
	{	
	for (d = 0 ; d < 10 ; d++)
	{
	putchar (a + '0');
	putchar (b + '0');
	putchar (' ');
	putchar (c + '0');
	putchar (d + '0');
